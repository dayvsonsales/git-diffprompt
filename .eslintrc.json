{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["prettier", "eslint-config-prettier"],
  "rules": {
    "prettier/prettier": "error",
    "consistent-return": "off",
    "no-await-in-loop": "off"
  }
}
